        sensorValue  GBTFloat,
        sensorUpperThresholdNonCritical GBTFloat,
        sensorUpperThresholdCritical    GBTFloat,
        sensorLowerThresholdNonCritical GBTFloat,
        sensorLowerThresholdCritical    GBTFloat
